<launch>
    <arg name = "rate" default = "5" />
    <node
      name="voltafortest"
      pkg="volta"
      type="volta"
      args="$(arg rate)"
    />
  <node pkg="rosbag" type="play" name="playerImage" args="--loop $(find volta)/test/TestROSExploreTest.bag"/>
  <node pkg="rosbag" type="play" name="playerScan" args="--loop $(find volta)/test/imageTest.bag"/>
  <test
      test-name="voltaTest"
      pkg="volta"
      type="volta_test"
    />
</launch>
